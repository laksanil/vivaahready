{
  "name": "vivaahready",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node scripts/db-safety.mjs check && next dev",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "node scripts/db-safety.mjs prisma db push",
    "db:studio": "node scripts/db-safety.mjs prisma studio",
    "import": "node scripts/import-profiles.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test && npm run test:e2e",
    "test:ci": "vitest run --reporter=json --outputFile=test-results/unit.json && playwright test --reporter=json",
    "test:smoke": "playwright test --grep @smoke",
    "test:quick": "vitest run tests/unit && playwright test tests/e2e/navigation.spec.ts tests/e2e/api-health.spec.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@aws-sdk/client-sns": "^3.992.0",
    "@hookform/resolvers": "^3.4.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.14.0",
    "@stripe/stripe-js": "^3.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.8.0",
    "clsx": "^2.1.1",
    "encoding": "^0.1.13",
    "face-api.js": "^0.22.2",
    "googleapis": "^134.0.0",
    "lucide-react": "^0.378.0",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.4",
    "resend": "^6.7.0",
    "tailwind-merge": "^2.3.0",
    "web-push": "^3.6.7",
    "zipcodes": "^8.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/web-push": "^3.6.4",
    "@types/zipcodes": "^8.0.5",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "postcss": "^8.4.38",
    "prisma": "^5.14.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vitest": "^4.0.17"
  }
}
